@article{ WOS:A1994QV96100006,
Author = {LUMELSKY, VJ and SUN, K},
Title = {3-DIMENSIONAL MOTION PLANNING IN AN UNKNOWN ENVIRONMENT FOR ROBOT ARM
   MANIPULATORS WITH REVOLUTE OR SLIDING JOINTS},
Journal = {INTERNATIONAL JOURNAL OF ROBOTICS \& AUTOMATION},
Year = {1994},
Volume = {9},
Number = {4},
Pages = {188-198},
Abstract = {The existing work on nonheuristic motion planning of robot arm
   manipulators in unknown environments covers mostly various
   two-dimensional (2D) cases. Extending the algorithms to
   three-dimensional (3D) cases is important from both a theoretical and ar
   engineering standpoint. In this paper a motion-planning strategy is
   presented for a class of three-link robot arm manipulators operating in
   an unknown 2D or 3D environment. The joints of the robot arm manipulator
   can be either revolute or prismatic (sliding). Out of eight possible
   combinations of kinematic linkages, the approach applies to those four
   combinations for which the third joint is sliding. This covers the
   majority of arm manipulators used in practice. The authors show that the
   tree configuration space of such a robot arm has a 2D deformation
   retract that allows one to reduce the dimensionality of the
   motion-planning problem. Existing theory of motion planning is then
   extended appropriately, producing powerful algorithms that guarantee
   collision-free motion for the robot arm's whole body in a 3D environment
   with unknown obstacles of arbitrary shape.},
