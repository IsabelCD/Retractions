   Yang, Laurence T.},
Editor = {Min, G and Lefevre, L and Hu, J and Liu, L and Yang, LT and Seelam, S},
Book-Group-Author = {IEEE},
Title = {DMTRH: A Real-Time Scheduling Algorithm for Multiple Earth Observation
   Satellites},
Booktitle = {2012 IEEE 14TH INTERNATIONAL CONFERENCE ON HIGH PERFORMANCE COMPUTING
   AND COMMUNICATIONS \& 2012 IEEE 9TH INTERNATIONAL CONFERENCE ON EMBEDDED
   SOFTWARE AND SYSTEMS (HPCC-ICESS)},
