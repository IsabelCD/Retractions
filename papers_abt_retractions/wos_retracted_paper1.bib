@inproceedings{ WOS:000772157700009,
Author = {Addepalli, Amulya and Subin, Karen Ann and Schneider, Jodi},
Editor = {Smits, M},
Title = {Testing the Keystone Framework by Analyzing Positive Citations to
   Wakefield's 1998 Paper},
Booktitle = {INFORMATION FOR A BETTER WORLD: SHAPING THE GLOBAL FUTURE, PT I},
Series = {Lecture Notes in Computer Science},
