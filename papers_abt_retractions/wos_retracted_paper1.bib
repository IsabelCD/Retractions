@article{ WOS:000504302900041,
Author = {Alisic, Rijad and Pare, Philip E. and Sandberg, Henrik},
Title = {Modeling and Stability of Prosumer Heat Networks},
Journal = {IFAC PAPERSONLINE},
Year = {2019},
Volume = {52},
Number = {20},
Pages = {235-240},
Note = {8th IFAC Workshop on Distributed Estimation and Control in Networked
   Systems (NECSYS), Loyola Univ, Chicago, IL, SEP 16-17, 2019},
Organization = {Int Federat Automat Control, Tech Comm 1 5 Networked Syst; Int Federat
   Automat Control, Tech Comm 1 1 Modelling, Identificat \& Signal Proc;
   Int Federat Automat Control, Tech Comm 1 4 Stochast Syst; Int Federat
   Automat Control, Tech Comm 2 2 Linear Control Syst; Int Federat Automat
   Control, Tech Comm 2 3 Non Linear Control Syst; Int Federat Automat
   Control, Tech Comm 3 3 Telemat Control Commun Networks; Int Federat
   Automat Control, Tech Comm 6 4 Fault Detect, Supervis \& Safety Tech
   Proc; Int Federat Automat Control, Tech Comm 7 5 Intelligent Autonomous
   Vehicles},
Abstract = {The energy sector is going through a large transformation due to public
   demands of renewable energy sources. However, a major issue is that
   these energy sources are intermittent. If designed correctly, district
   heating systems can naturally contain energy storing units, for example
   by storing heat in the isolated pipes that make up the heat grid.
   Additionally, this makes it easier to reuse and transport already
   generated heat to other users. This paper proposes a mathematical model
   of such a grid, where excess energy can be retracted from one user and
   distributed to other users using a network of heat pumps. In some cases,
   one can balance residual heat production with the heat consumption,
   temporarily eliminating the need for a centralized heating plant.
   Existence conditions for stable steady states of such a network with
   general topology are given. Finally, energy optimal stable steady states
   are obtained through convex optimization. Copyright (C) 2019. The
   Authors. Published by Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
