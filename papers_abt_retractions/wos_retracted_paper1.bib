@article{ WOS:000261291500002,
Author = {Viret, S. and Parkes, C. and Gersabeck, M.},
Title = {Alignment procedure of the LHCb vertex detector},
Journal = {NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT},
Year = {2008},
Volume = {596},
Number = {2},
Pages = {157-163},
Month = {NOV 1},
Abstract = {LHCb is one of the four main experiments of the Large Hadron Collider
   (LHC) project, which will start at CERN in 2008. The experiment is
   primarily dedicated to B-Physics and hence requires precise vertex
   reconstruction. The silicon vertex locator (VELO) has a single hit
   precision of better than 10 mu m and is used both off-line and in the
   trigger. These requirements place strict constraints on its alignment.
   Additional challenges for the alignment arise from the detector being
   retracted between each fill of the LHC and from its unique circular disc
   r/phi strip geometry. This paper describes the track-based software
   alignment procedure developed for the VELO. The procedure is primarily
   based on a non-iterative method using a matrix inversion technique. The
   procedure is demonstrated with simulated events to be fast, robust and
   to achieve a suitable alignment precision. (C) 2008 Elsevier B.V. All
   rights reserved.},
