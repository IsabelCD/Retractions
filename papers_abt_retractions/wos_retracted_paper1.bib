@article{ WOS:000169805200006,
Author = {Bebchuk, LA and Ben-Shahar, O},
Title = {Precontractual reliance},
Journal = {JOURNAL OF LEGAL STUDIES},
Year = {2001},
Volume = {30},
Number = {2, 1},
Pages = {423-457},
Month = {JUN},
Abstract = {During contractual negotiations, parties often make reliance
   expenditures that would increase the surplus should a contract be made.
   This paper analyzes decisions to invest in precontractual reliance under
   alternative legal regimes. Investments in reliance will be socially
   suboptimal in the absence of any precontractual liability-and will be
   socially excessive under strict liability for all reliance expenditures.
   Given the results for these polar cases, we focus on exploring how
   ``intermediate{''}-liability rules could be best designed to induce
   efficient reliance decisions. One of our results indicates that the case
   for liability is shown to be stronger when a party retracts from terms
   that it has proposed or from preliminary understandings reached by the
   parties. Our results have implications, which we discuss, for various
   contract doctrines and debates. Finally, we show that precontractual
   liability does not necessarily have an overall adverse effect on
   parties' decisions to enter into contractual negotiations.},
