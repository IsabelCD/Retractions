@article{ WOS:000559224700005,
Author = {Bhavale, A. N. and Waphare, B. N.},
Title = {Basic retracts and counting of lattices},
Journal = {AUSTRALASIAN JOURNAL OF COMBINATORICS},
Year = {2020},
Volume = {78},
Number = {1},
Pages = {73-99},
Abstract = {In this paper, we treat nullity of a poset as the nullity of its cover
   graph. Using nullity, we introduce the concepts of a basic retract
   associated to a poset and a fundamental basic block associated to a
   dismantlable lattice. We prove that a lattice in which all the reducible
   elements are comparable is dismantlable but the converse is not true. We
   establish recurrence relations and count up to isomorphism all
   fundamental basic blocks and all basic retracts/blocks associated to the
   lattices (with respect to arbitrary large nullity and/or the number of
   reducible elements) in which all the reducible elements are comparable.},
