@article{ WOS:000392176800008,
Author = {Ebrahimi, M. Mehdi and Mahmoudi, Mojgan and Yavari, Mahdieh},
Title = {Absolute retractness of automata on directed complete posets},
Journal = {JOURNAL OF ALGEBRA AND ITS APPLICATIONS},
Year = {2017},
Volume = {16},
Number = {1},
Month = {JAN},
Abstract = {The notion of retractness, which is about having left inverses
   (reflection) for monomorphisms, is crucial in most branches of
   mathematics. One very important notion related to it is injectivity,
   which is about extending morphisms to larger domains and plays a
   fundamental role in many areas of mathematics as well as in computer
   science, under the name of complete or partial objects. Absolute
   retractness is tightly related to injectivity and is in fact equivalent
   to it in many categories. In this paper, combining the two important
   notions of actions of semigroups and directed complete posets, which are
   both crucial abstraction and useful in mathematics as well as in
   computer science, we consider the category Dcpo-S of actions of a
   directed complete semigroup on directed complete posets, and study
   absolute retractness with respect to both monomorphisms and embeddings
   in this category. Among other things, we show that absolute retract
   (S-)dcpo's are complete but the converse is not necessarily true.
   Investigating the converse, we find that if we add the property of being
