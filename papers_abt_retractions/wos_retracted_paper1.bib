@article{ WOS:000075548600001,
Author = {Andre, RP},
Title = {On the supremum of a family of singular compactifications},
Journal = {ROCKY MOUNTAIN JOURNAL OF MATHEMATICS},
Year = {1998},
Volume = {28},
Number = {1},
Pages = {1-24},
Month = {SPR},
Abstract = {Singular compactifications of locally compact Hausdorff spaces were
   first introduced over a decade ago. An elegant characterization of
   singular compactifications is the following: A compactification alpha X
   of the locally compact Hausdorff space X is singular if and only if
   alpha X\textbackslash{}X is a retract of alpha X. In this paper we
   provide a new representation of singular compactifications. It has been
   previously shown that the supremum of singular compactifications need
   not itself be a singular compactification. Examples of this fact are
   easy to find. We provide necessary and sufficient conditions which
   describe when the supremum of a family of singular compactifications is
   a singular compactification. We also show that there are
   compactifications which are not the supremum of a family of singular
   compactifications. For two singular functions f and g such that S(f) is
   homeomorphic to S(g) we describe conditions which show when X boolean
   ORf S(f) is equivalent to X boolean ORg S(g).},
