@article{ WOS:000975973900001,
Author = {da Silva, Jaime A. Teixeira and Nazarovets, Serhii},
Title = {Assessment of retracted papers, and their retraction notices, from a
   cancer journal associated with ``paper mills{''}},
Journal = {JOURNAL OF DATA AND INFORMATION SCIENCE},
Year = {2023},
Volume = {8},
Number = {2},
Pages = {118-125},
Month = {APR 1},
Abstract = {Cancer research is occasionally described as being in a reproducibility
   crisis. The cancer literature has ample papers retracted due to
   misconduct, including the use of paper mills, invalid authorship, or
   fake data. The objective of this paper was to gain an appreciation of
   the balance of retractions and associated retraction notices of 23
   retracted Cancer Biotherapy and Radiopharmaceuticals papers associated
   with paper mills. By 23 March 2023, these retracted papers had already
   accumulated 287 citations according to Web of Science Core Collection,
   253 according to Scopus, and 365 according to Google Scholar, i.e.,
   metrically speaking, they were highly rewarded. All authors had an
   affiliation (71\% being a hospital) in China. Most (12/21; 57\%) of
   corresponding authors had emails with a @163.com suffix. Four of the
   retraction notices (i.e., 17\%) explicitly indicated paper mills as a
   reason for retraction although, in general, the retraction notices
   lacked details and background that could assist readers' understanding
   of the retractions.},
