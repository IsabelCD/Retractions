@article{ WOS:000621750900005,
Author = {Schenk, Kai and Lunze, Jan},
Title = {COCO - an experimental setup for testing networked control methods},
Journal = {AT-AUTOMATISIERUNGSTECHNIK},
Year = {2021},
Volume = {69},
Number = {2},
Pages = {131-141},
Month = {FEB},
Abstract = {This paper describes a new experimental setup for the purpose of testing
   control methods for networked systems. A flexible surface is formed by
   fifty linear actuators mounted under a latex foil. By retracting and
   extending the cylinders, different shapes can be imprinted on the latex
   foil. The control of the actuators consists of synchronizing
