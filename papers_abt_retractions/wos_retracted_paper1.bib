@inproceedings{ WOS:000709638700073,
Author = {Leta, Jacqueline and Araujo, Kizi and Treiber, Stephanie},
Editor = {Glanzel, W and Heeffer, S and Chi, PS and Rousseau, R},
Title = {Citing a retracted paper: the case of Wakefield's article that
   correlates vaccine and autism},
