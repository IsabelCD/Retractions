@article{ WOS:000399160700008,
Author = {Zhao, Haoran and Kou, Hui},
Title = {A stable universal domain related to T-omega},
Journal = {MATHEMATICAL STRUCTURES IN COMPUTER SCIENCE},
Year = {2017},
Volume = {27},
Number = {4},
Pages = {540-556},
Month = {MAY},
Note = {6th International Symposium on Domain Theory (ISDT), Campus Hunan Univ,
   Changsha, PEOPLES R CHINA, OCT 25-29, 2013},
Abstract = {In 1978, G. Plotkin noticed that T-omega, the cartesian product of omega
   copies of the three element flat domain of Booleans, is a universal
   domain, where `universal' means that the retracts of T-omega for Scott's
   continuous semantics are exactly all the omega CC-domains, which with
   Scott continuous functions form a cartesian closed category. As usual,
   `omega' is for `countably based,' and here `CC' is for `conditionally
   complete,' which essentially means that any subset which is pairwise
   bounded has a least upper bound. Since T-omega is also an omega
   DI-domain (an important structure in stable domain theory), the
   following problem arises naturally: is there a cartesian closed category
   C of domains with stable functions such that T-omega, or a related
   structure, is universal in C for Berry's stable semantics? The aim of
   this paper is to answer this question. We first investigate the
   properties of stable retracts. We introduce a new class of domains
   called conditionally complete DI-domains (CCDI-domain for short) and
   show that, (1) T-omega is an omega CCDI-domain and the category of
   CCDI-domains (resp.omega CCDI-domains) with stable functions is
   cartesian closed; (2) {[}T-omega ->(st) T-omega] is a stable universal
   domain in the sense that every omega CCDI-domain is a stable retract of
   {[}T-omega -> (st) T-omega], where {[}T-omega -> (st) T-omega] is the
   stable function space of T-omega; (3) in particular, {[}T-omega ->(st)
   T-omega] is not a stable retract of T-omega and hence T omega is not
   universal for Berry's stable semantics. We remark that this paper is a
   completion and correction of our earlier report in the Proceedings of
   the 6th International Symposium on Domain Theory and Its Applications
   (ISDT2013).},
