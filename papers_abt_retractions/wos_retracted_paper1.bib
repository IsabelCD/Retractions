@article{ WOS:001021839500013,
Author = {Pham, Tien and Nugroho, Anda},
Title = {Tourism-induced poverty impacts of COVID-19 in Indonesia},
Journal = {ANNALS OF TOURISM RESEARCH EMPIRICAL INSIGHTS},
Year = {2022},
Volume = {3},
Number = {2},
Month = {NOV},
Abstract = {International tourism is an important sector in developing countries for
   its ability to alleviate poverty. Impacts of COVID-19 are detrimental to
   all tourism destinations. This paper examines the pandemic effects on
