@article{ WOS:000179957200006,
Author = {Saltman, DJ and Tignol, JP},
Title = {Generic algebras with involution of degree 8m},
Journal = {JOURNAL OF ALGEBRA},
Year = {2002},
Volume = {258},
Number = {2},
Pages = {535-542},
Month = {DEC 15},
Abstract = {The centers of the generic central simple algebras with involution are
   interesting objects in the theory of central simple algebras. These
   fields also arise as invariant fields for linear actions of projective
   orthogonal or symplectic groups. In this paper, we prove that when the
   characteristic is not 2, these fields are retract rational, in the case
   the degree is 8m and m is odd. We achieve this by proving the equivalent
   lifting property for the class of central simple algebras of degree 8m
