@article{ WOS:A1996WG11400002,
Author = {Bernelli-Zazzera, F and ErcoliFinzi, A and Mantegazza, P},
Title = {Attitude maneuvers of a flexible space station by means of deployable
   tethers},
Journal = {JOURNAL OF THE ASTRONAUTICAL SCIENCES},
Year = {1996},
Volume = {44},
Number = {3},
Pages = {293-313},
Month = {JUL-SEP},
Note = {4th International Conference on Tethers in Space, WASHINGTON, D.C., APR,
   1995},
Abstract = {This paper presents a possible approach to the study of attitude
   maneuvers of a deformable space station equipped with deployable
   tethers. Since the equilibrium attitude depends on the tether lengths,
   it is demonstrated that stable maneuvers can be carried out by deploying
   or retracting the tethers. The equations of motion of the space station
   are derived via the application of the virtual work principle to each
   element of the space station, including tethers. A finite element model
   is used to represent both the tethers and the deformable elements of the
   space station. The latter can be easily inherited from any standard
   finite element code while a special model is developed for the tethers
   in order to specifically include the effects of their variation.
   Numerical simulations are carried out on an abstract model of the space
   station in order to demonstrate the feasibility of stable roll attitude
   maneuvers and to define the range of achievable stable attitudes.},
