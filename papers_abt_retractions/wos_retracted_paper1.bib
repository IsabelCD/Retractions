Title = {Long-range automatic precision displacement measuring of winding system
   using double timing belt transmission},
Booktitle = {TENTH INTERNATIONAL SYMPOSIUM ON PRECISION ENGINEERING MEASUREMENTS AND
   INSTRUMENTATION},
Series = {Proceedings of SPIE},
