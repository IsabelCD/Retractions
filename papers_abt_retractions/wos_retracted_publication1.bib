@article{ WOS:000295368300014,
Author = {Leri, Annarosa and Kajstura, Jan and Anversa, Piero},
Title = {RETRACTED: Role of Cardiac Stem Cells in Cardiac Pathophysiology: A
   Paradigm Shift in Human Myocardial Biology (Publication with Expression
   of Concern. See vol. 124, 2019) (Publication with Expression of Concern.
   See vol. 124, 2019) (Retracted article. See vol. 124, 2019)},
Journal = {CIRCULATION RESEARCH},
Year = {2011},
Volume = {109},
