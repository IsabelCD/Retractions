@article{ WOS:000471238300030,
Author = {Bolboaca, Sorana D. and Buhai, Diana-Victoria and Aluas, Maria and
   Bulboaca, Adriana E.},
Title = {Post retraction citations among manuscripts reporting a
   radiology-imaging diagnostic method},
Journal = {PLOS ONE},
Year = {2019},
Volume = {14},
Number = {6},
Month = {JUN 13},
Abstract = {Our study aimed to evaluate the trends of post retraction citations of
   articles reporting a radiology-imaging diagnostic method and to find if
   a different pattern exists between manuscripts reporting an ultrasound
   method and those reporting other radiology diagnostic methods. This
   study reviewed retractions stored in PubMed on the subject of
   radiology-imaging diagnosis to identify the motivation, time from
   publication to retraction, and citations before and after retraction.
   The PubMed database was searched on June 2017 to retrieve the retracted
   articles, and the Scopus database was screened to identify the
   post-retraction citations. The full text was screened to see the type of
   post-retraction citation (positive/negative) and whether the cited
   article appears or not as retracted. One hundred and two retractions
   were identified, representing 3.5\% of the retracted articles indexed by
   PubMed, out of which 54 were included in the analysis. Half of the
   articles were retracted in the first 24 months after publication, and
   the number of post retraction citations was higher than the number of
   citations before retraction in 30 out of 54 cases (US methods: 9/20,
   other diagnostic methods 21/34, P-value = 0.2312). The plagiarism was
