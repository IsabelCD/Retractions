@inproceedings{ WOS:000709638700056,
Author = {Haunschild, Robin and Bornmann, Lutz},
Editor = {Glanzel, W and Heeffer, S and Chi, PS and Rousseau, R},
Title = {Early detection of problems with scientific papers using Twitter data: A
   case study of three retracted COVID-19/SARS-CoV-2 papers},
Booktitle = {18TH INTERNATIONAL CONFERENCE ON SCIENTOMETRICS \& INFORMETRICS
   (ISSI2021)},
