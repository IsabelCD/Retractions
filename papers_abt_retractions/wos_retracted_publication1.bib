@article{ WOS:000482302200014,
Author = {Chen, Yangkang and Chen, Wei and Wang, Yufeng and Bai, Min},
Title = {RETRACTED: Least-squares decomposition with time-space constraint for
   denoising microseismic data (Publication with Expression of Concern. See
   vol. 221, pg. 2055, 2020) (Retracted article. See vol. 222, pg. 1896,
   2020)},
Journal = {GEOPHYSICAL JOURNAL INTERNATIONAL},
Year = {2019},
Volume = {218},
Number = {3},
Pages = {1702-1718},
Month = {SEP},
Abstract = {Microseismic data are usually of low signal-to-noise ratio (SNR), which
   makes it difficult to utilize the microseismic waveforms for imaging and
   inversion. We develop a useful denoising algorithm based on a
   non-stationary least-squares decomposition model to enhance the quality
   of microseismic signals. The microseismic signals are assumed to be
   represented by a superposition of several smoothly variable components.
   We construct a least-squares inverse problem to solve for the the smooth
   components. We constrain the least-squares inversion via both time and
   space constraints. The temporal smoothness constraint is applied to
   ensure the stability when calculating the non-stationary autoregression
   coefficients. The space-smoothness constraint is applied to extract the
   spatial correlation among multichannel microseismic traces. The new
   algorithm is validated via several synthetic and real microseismic data
   and are proved to be effective. Comparison with the state-of-the-art
   algorithms demonstrates that the proposed method is more powerful in
   suppressing random noise of a wide range of levels than its competing
   methods.},
