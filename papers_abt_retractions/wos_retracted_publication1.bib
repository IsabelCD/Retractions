@article{ WOS:000241862000015,
Author = {Seuwen, Klaus and Ludwig, Marie-Gabrielle and Wolf, Romain M.},
Title = {Review - Receptors for protons or lipid messengers or both?},
Journal = {JOURNAL OF RECEPTORS AND SIGNAL TRANSDUCTION},
Year = {2006},
Volume = {26},
Number = {5-6},
Pages = {599-610},
Note = {10th Swiss Receptor Workshop, Univ Basel, Pharmacenter, Basel,
   SWITZERLAND, MAR 12-15, 2006},
Abstract = {The subfamily of G protein-coupled receptors comprising GPR4, OGR1,
   TDAG8, and G2A was originally characterized as a group of proteins
   mediating biological responses to the lipid messengers
   sphingosylphosphoryleholine (SPC), lysophosphatidylcholine (LPC), and
   psychosine. We challenged this view by reporting that OGR1 and GPR4
   sense acidic pH and that this process is not affected by concentrations
   of SPC or LPC previously reported as agonistic. The original
   publications describing GPR4, OGR1, and G2A as receptors for LPC or SPC
   have now been retracted, and the first studies exploring receptors of
   this family as pH sensors in physiology have appeared. Here we review
   the status of this field and we confirm that GPR4, OGR1, and TDAG8
   should be considered as proton-sensing receptors. Negative regulation of
   these receptors by high micromolar concentrations of lipids appears not
   specific in our experiments.},
