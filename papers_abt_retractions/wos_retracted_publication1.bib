@article{ WOS:000235044700016,
Author = {Dahlberg, JE and Mahler, CC},
Title = {The Poehlman case: running away from the truth},
Journal = {SCIENCE AND ENGINEERING ETHICS},
Year = {2006},
Volume = {12},
